# .gitignore for MERN stack (TypeScript variant)
# Generated to ignore common Node, frontend, and TypeScript artifacts

# Node dependencies
node_modules/

# Build / output
dist/
build/
out/
public/build/
.next/
.cache/
.parcel-cache/

# TypeScript
*.tsbuildinfo
tsconfig.tsbuildinfo

# Environment / secrets
.env
.env.*.local
.env.local
.env.development.local
.env.test.local
.env.production.local
secrets/

# Logs
logs/
*.log
npm-debug.log*
yarn-debug.log*
pnpm-debug.log*

# Coverage
coverage/
.nyc_output/
jest-coverage/

# Test reports
test-results.xml
reports/

# Runtime
pids/
*.pid
*.pid.lock
tmp/
temp/
*.tmp

# Dependency folders (common monorepo-like locations)
client/node_modules/
server/node_modules/

# IDEs and editors
.vscode/
.idea/
*.iml

# OS files
.DS_Store
Thumbs.db
ehthumbs.db

# Docker
.docker/
docker-compose.override.yml

# Compiled source
*.js
*.jsx
*.cjs
*.mjs
*.map

# Archives
*.zip
*.tar.gz

# Misc
*.log.*
.cache

# Optional: lockfile for plug'n'play
.pnp.*

